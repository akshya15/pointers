#include <stdio.h>
int findMax(int *arr, int n) {
int max = *arr;
for (int i = 1; i < n; i++) {
if (*(arr + i) > max) {
max = *(arr + i);
}
}
return max;
}
int main() {
int arr[] = {10, 25, 3, 45, 89, 2};
int n = sizeof(arr) / sizeof(arr[0]);
printf("Maximum element is %d\n", findMax(arr, n));
return 0;
}
